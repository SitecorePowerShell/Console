<?xml version="1.0" encoding="utf-8"?>

<control xmlns:def="Definition" xmlns="http://schemas.sitecore.net/Visual-Studio-Intellisense">
  <PowerShellResultViewerText>
    <FormPage>
      <Stylesheet Src="/sitecore/shell/themes/standard/default.css" DeviceDependant="true" runat="server" />
      <Stylesheet Src="/Console/Styles/jquery.ui.all.css" DeviceDependant="true" runat="server" />
      <Stylesheet Src="/Console/Styles/Console.css" DeviceDependant="true" runat="server" />
      <CodeBeside Type="Cognifide.PowerShell.SitecoreIntegrations.Applications.PowerShellResultViewerText,Cognifide.PowerShell" />
      <Scrollbox ID="All" class="consoleWrapper console pre">
        <Scrollbox ID="Promo" class="promoArea console pre">
          <Border Align="Right" Foreground="#999999">
            <Literal
              Text="Sitecore PowerShell Console - Copyright (c) 2010-2013 &lt;a href='http://www.cognifide.com' target='_blank'&gt;Cognifide Limited&lt;/a&gt; &amp; &lt;a href='http://blog.najmanowicz.com/' target='_blank'&gt;Adam Najmanowicz&lt;/a&gt;." />
          </Border>
        </Scrollbox>
        <Literal ID="Result" class="resultsArea console pre"/>
      </Scrollbox>
    </FormPage>
  </PowerShellResultViewerText>
</control>